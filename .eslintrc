{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 0,
    "react/prop-types": 0,
    "no-irregular-whitespace": 0,
    "no-unused-expressions": 0,
    "react/destructuring-assignment": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 2,
        "semi": false
      }
    ]
  }
}
